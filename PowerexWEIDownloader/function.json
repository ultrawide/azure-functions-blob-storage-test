{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "name": "mytimer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "0 0 */1 * * *"
    },
    {
      "name": "outputBlob",
      "type": "blob",
      "direction": "out",
      "connection": "AzureWebJobsStorage",
      "path": "blob/test.txt"
    }
  ]
}
